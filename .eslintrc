{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error", "info"] }]
  },
  "env": {
      "browser": false,
      "node": true,
      "es6": true,
      "mocha": true
  }
}
